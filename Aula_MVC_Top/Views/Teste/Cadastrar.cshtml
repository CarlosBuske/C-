@model Aula_MVC_Top.Models.Vendedor
@{
    ViewBag.Title = "Cadastrar";
}

<h2>Cadastrar</h2>
@*@usiing (Html.BeginForm("Cadastrar", "Vendedor", FormMethod.Post, new { enctype= "multipart/form-data" })){ }*@
<form method="post">
    @Html.Label("Codigo")
    <br />
    @Html.TextBoxFor(x => x.cod)
    <br />
    @Html.ValidationMessageFor(x => x.cod)
    <br />

    @Html.Label("Nome")
    <br />
    @Html.TextBoxFor(x => x.Nome)
    <br />
    @Html.ValidationMessageFor(x => x.Nome)
    <br />

    @Html.Label("Salario Fixo")
    <br />
    @Html.TextBoxFor(x => x.salario_fixo)
    <br />
    @Html.ValidationMessageFor(x => x.salario_fixo)
    <br />

    @Html.Label("Faixa Comissão")
    <br />
    @Html.TextBoxFor(x => x.faixa_comissao)
    <br />
    @Html.ValidationMessageFor(x => x.faixa_comissao)
    <br />

    @Html.Label("Sexo")
    <br />
    @Html.TextBoxFor(x => x.sexo)
    <br />
    @Html.ValidationMessageFor(x => x.sexo)
    <br />



    <br />
    <input type="submit" value="Salvar" />

</form>
@section Scripts{
    @Scripts.Render("~/bundles/jqueryval")


}